{
  "database": {
    "rules": "firebase-rules-phase1-fixed.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/ttmd",
        "destination": "/index.html"
      },
      {
        "source": "/ttmd/**",
        "destination": "/index.html"
      }
    ]
  }
}
